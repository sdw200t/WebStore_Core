@model Employee

<!DOCTYPE html>

<div class="container">
    <h3>Сотрудник</h3>
    <dl class="dl-horizontal">
        <dt>Id</dt>
        <dd>
            @Model.Id
        </dd>
        <dt>LastName</dt>
        <dd>
            @Model.LastName
        </dd>
        <dt>FirstName</dt>
        <dd>
            @Model.FirstName
        </dd>
        <dt>Age</dt>
        <dd>
            @Model.Age
        </dd>
    </dl>
    <hr />
    <a asp-action="Index" class="btn btn-default">К списку</a>
</div>